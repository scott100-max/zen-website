    },
    {
      "index": 41,
      "type": "silence",
      "start_time": 287.9,
      "duration": 5,
      "end_time": 292.9
    },
    {
      "index": 42,
      "type": "text",
      "start_time": 292.899816,
      "text": "Or your chest, where you feel the gentle rise and fall.",
