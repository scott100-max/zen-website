      "expected": 5.4,
      "end_time": 201.29
    },
    {
      "index": 31,
      "type": "silence",
      "start_time": 201.29,
      "duration": 5,
      "end_time": 206.29
    },
    {
      "index": 32,
      "type": "text",
      "start_time": 206.28694000000002,
      "text": "You're not doing anything now except this.",
